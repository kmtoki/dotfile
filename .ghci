:set prompt "Î» "

:def b \x -> return $ ":break " ++ x
:def c \x -> return $ ":contiune " ++ x
:def s \x -> return $ ":step " ++ x

:def hoogle \x -> return $ ":!hoogle '" ++ x ++ "'"
:def h      \x -> return $ ":!hoogle --count=20 '" ++ x ++ "'"
:def doc    \x -> return $ ":!hoogle --info '" ++ x ++ "'"
:def alc    \x -> return $ ":!eijiro '" ++ x ++ "'"
:def pf     \x -> return $ ":!pointfree '" ++ x ++ "'"

